{
  "name": "chika-component",
  "version": "0.0.2",
  "description": "A react-component collection, IQ3 でもまかせなさいー！",
  "main": "lib/index.js",
  "scripts": {
    "start": "nino go -e ./src/go",
    "test": "nino test",
    "build": "nino koei -c ./scripts/nino.koei.js",
    "compile": "nino compile",
    "codecov": "nino test -d",
    "dev": "nino koei -w-d",
    "lint": "npx eslint .",
    "prettier": "nino prettier",
    "lint-staged": "lint-staged",
    "backup": "npm config set registry=https://registry.npmjs.org",
    "pub": "npm run build && npm run compile && git push origin master && npm publish && npm config set registry=https://registry.npm.taobao.org"
  },
  "files": [
    "lib",
    "dist"
  ],
  "dependencies": {
    "antd-mobile": "^2.2.11",
    "leaflet": "^1.4.0",
    "nino-cli": "^0.6.6"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.7",
    "@types/leaflet": "^1.4.3",
    "@types/react": "^16.8.8",
    "@types/react-dom": "^16.8.3",
    "@types/react-router-dom": "^4.3.1",
    "@typescript-eslint/parser": "^1.5.0",
    "date-fns": "^2.0.0-alpha.27",
    "dekko": "^0.2.1",
    "enzyme": "^3.9.0",
    "eslint": "^5.15.3",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "moment": "^2.24.0",
    "pre-commit": "^1.2.2",
    "react-router-dom": "^5.0.0",
    "whatwg-fetch": "^3.0.0"
  },
  "license": "MIT",
  "pre-commit": [
    "lint-staged"
  ],
  "lint-staged": {
    "*.{ts,tsx}": [
      "npm run prettier",
      "git add"
    ]
  }
}
